/**index.wxss**/
@import "../../component/footermenu/footermenu.wxss";
.container{
  width:100%;
  position: absolute;
  display:flex;
  
}
.index-content{
  position:absolute;
  width: 90%;
  margin-left: 5%;
  height: 15%;
  justify-content: center;
}
.content-info{
  height: 50%;
  display: flex;
  color:#aaaaaa;
  justify-content: center;
  align-items: center;
}
.content-title{
  height: 50%;
  background-color: #55b737;
  width: 100%;
  border-radius: 3px;
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
}
.header-index{
  height: 80px;
  width: 100%;
  background-color: #55b737;
  /* border: 1px solid black; */
}
.header-index-detail{
  height: 80px;
  width: 90%;
  /* border: 1px solid white; */
  margin: 0 8%;
  display: flex
}
.header-index-moudle{
  color: #fff;
  height: 100%;
  width: 33%;
  /* border: 1px solid white; */
}
.header-index-moudle-title{
  font-size: 12px;
  line-height: 15px;
  height: 15px;
  /* border: 1px solid white; */
}
.header-index-moudle-info{
  display: flex;
  margin-top:15px;
}
.info-big{
  /* border: 1px solid white; */
  font-size: 30px
}
.info-little{
  /* border: 1px solid white; */
  font-size: 12px;
  margin-top: 17px
}
.actioner{
  margin-top: 15px
}
/* index-content */
.index-content{
  margin-top: 0px
}
.margin-bottom-xl{
  margin-bottom: 0px;
  position: relative;
  z-index: 0;
} 